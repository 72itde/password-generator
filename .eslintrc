{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "parserOptions": { "ecmaVersion": 11 },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "webpack.common.js",
          "webpack.dev.js",
          "webpack.prod.js"
        ]
      }
    ],
    "no-underscore-dangle": "off",
    "sort-imports": [
      "error",
      {
        "ignoreCase": true,
        "ignoreMemberSort": false,
        "memberSyntaxSortOrder": ["none", "all", "multiple", "single"]
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.dev.js"
      }
    }
  }
}
